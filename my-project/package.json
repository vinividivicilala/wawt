{
  "name": "user-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "dev:css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "dev": "npm-run-all --parallel watch:ts dev:css",
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^4.9.5"
  }
}
