{
  "name": "user-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css",
    "dev:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "dev:next": "next dev",
    "build:next": "next build",
    "start:next": "next start",
    "lint": "next lint",
    "dev": "npm-run-all --parallel watch:ts dev:css dev:next"
  },
  "dependencies": {
    "next": "^15.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^4.9.5"
  }
}
